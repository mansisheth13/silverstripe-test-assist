{
	"name": "symbiote/silverstripe-test-assist",
	"description": "Some extensions to the default SS test structures to allow parametrisation in CI builds, as well as a framework for selenium testing.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "testing", "selenium", "continuous integration"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Marcus Nyeholt",
			"email": "marcus@symbiote.com.au"
		}
	],
	"require": {
		"silverstripe/framework": ">=3.1.7 <4.0"
	},
	"extra": {
		"installer-name": "test-assist",
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	},
	"replace": {
		"silverstripe-australia/ssautesting": "self.version"
	},
	"minimum-stability": "dev"
}
